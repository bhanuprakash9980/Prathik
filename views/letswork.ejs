    <form id="letswork-form" action="/lets" method="POST">
      
          <div class="form-group">
            <label for="exampleFormControlSelect1">Are You a business owner looking for marketing services ?</label>
            <select class="form-control " id="exampleFormControlSelect1" name="looking" style="width: auto;" required>
              <option selected>Please choose</option>
              <option  value="Yes">Yes</option>
              <option  value="No">No</option>
             </select>
          </div>
        <div class="form-group">
          <label for="exampleFormControlInput1">What is your Name?</label>
          <input type="text" class="form-control" id="exampleFormControlInput1" name="Name" required>
        </div>
        <div class="form-group">
            <label for="exampleFormControlInput1">What is the name of your company?</label>
            <input type="text" class="form-control" id="exampleFormControlInput1" name="name_company" required>
          </div>
          <div class="form-group">
            <label for="exampleFormControlInput1">What is the URL of your website?</label>
            <input type="url" class="form-control" id="exampleFormControlInput1" name="website_url" placeholder="https://example.com"   >
          </div>
        <div class="form-group">
          <label for="exampleFormControlTextarea1">Describe your Business , Product Or Service</label>
          <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="desc_comp" style="resize: none;"></textarea>
        </div>
       
        <div class="form-group">
            <label for="exampleFormControlSelect2">What is your main Priority right now?</label>
            <select class="form-control " id="exampleFormControlSelect2" name="priority" style="width: auto;" required>
            <option selected>Please choose</option>
              <option  value="Increasing Sales">Increasing Sales</option>
              <option  value="Brand Awareness">Brand Awareness</option>
              <option  value="Brand Awareness">Lead Generation</option>
             </select>
          </div>
        <div class="form-group">
            <label for="exampleFormControlInput1">What is your biggest hurdle currently?</label>
            <input type="text" class="form-control" id="exampleFormControlInput1" name="hurdle" required>
          </div>
          <div class="form-group">
            <label for="exampleFormControlInput1">What is your Marketing Budget?</label>
            <input type="text" class="form-control" id="exampleFormControlInput1" name="budjet" required>
          </div>
          <div class="form-group">
            <label for="exampleFormControlInput1">What is your Email ID?</label>
            <input type="email" class="form-control" id="exampleFormControlInput1" name="email" required>
          </div>
          <div class="form-group">
            <label for="exampleFormControlInput1">What is your Contact number</label>
            <input type="tel" class="form-control" id="exampleFormControlInput1" name="phone" required>
          </div>
        <input type="submit" value="submit" class="btn btn-primary btn-lg ">
